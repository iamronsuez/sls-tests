{
  "name": "is-mutant",
  "version": "1.0.0",
  "description": "",
  "main": "handler.js",
  "directories": {
    "lib": "lib"
  },
  "dependencies": {
    "aws-sdk": "2.534.0",
    "lodash.get": "4.4.2",
    "mongoose": "5.7.1",
    "serverless-offline": "5.11.0"
  },
  "devDependencies": {
    "eslint": "6.4.0",
    "eslint-config-standard": "14.1.0",
    "eslint-plugin-import": "2.18.2",
    "husky": "3.0.5",
    "jest-coverage-badges": "1.1.2",
    "lint-staged": "9.3.0",
    "mocha": "6.2.0",
    "npm-run-all": "4.1.5",
    "nyc": "14.1.1",
    "serverless-bundle": "1.2.5",
    "webpack-node-externals": "1.7.2"
  },
  "scripts": {
    "test": "serverless-bundle test --env node",
    "cov": "serverless-bundle test --coverage  --reporters default --env node && jest-coverage-badges",
    "lint:js": "eslint . --ext .js",
    "lint:js:fix": "npm run lint:js -- --fix"
  },
  "jest": {
    "collectCoverageFrom": [
      "functions/**/*.js",
      "lib/**/*.js",
      "./handler.js"
    ],
    "coverageReporters": [
      "json-summary"
    ]
  },
  "lint-staged": {
    "./**/*.{js}": [
      "npm run --silent lint:js:fix", "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm-run-all lint:js:fix cov"
    }
  },
  "author": "Ronald Suez <ingrsuezinfo@gmail.com>",
  "license": "MIT"
}
